{
    "manifest_version": 2,
   
    "name": "Index.hu Értesítő",
    "version": "1.0.0",
    "description": "Kapj asztali értesítéseket a élő oldalain.",

    "minimum_chrome_version": "49",

    "permissions": [
      "*://*.index.hu/*"
    ],

    "content_scripts": [{
      "js": ["scripts/content.js"],
      "matches": [ "*://*.index.hu/*" ]
    }]
  }